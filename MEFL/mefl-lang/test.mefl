// Function to greet ethically
function greetEthical(name) {
    if (isEthicalAction("greet", name)) {
        print("Hello, " + name + "!");
        return "Greeted ethically";
    } else {
        print("Cannot greet unethically.");
        return "Ethical greeting failed";
    }
}

